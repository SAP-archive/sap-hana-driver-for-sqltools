{
  "name": "sap-hana-driver-for-sqltools",
  "displayName": "SAP HANA Driver for SQLTools",
  "description": "A Visual Studio Code extension which extends SQLTools extension, with a driver to work with SAP HANA Database",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "SAPOSS",
  "license": "Apache-2.0",
  "repository": "https://github.com/SAP/sap-hana-driver-for-sqltools",
  "bugs": {},
  "scripts": {
    "ci": "npm-run-all lint compile package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint . --ext .ts --fix --ignore-path=.gitignore",
    "package": "vsce package .",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode developme  nt --watch"
  },
  "keywords": [
    "SAPHana",
    "sqltools-driver"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./dist/extension.js",
  "dependencies": {
    "@sqltools/base-driver": "0.1.8",
    "@sqltools/types": "0.1.5",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.5.5",
    "@types/jest": "24.0.23",
    "@types/node": "14.0.6",
    "@types/react-native": "0.62.12",
    "@types/vscode": "1.42.0",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.6.1",
    "eslint": "7.4.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "npm-run-all": "4.1.5",
    "ts-loader": "8.0.1",
    "typescript": "3.9.7",
    "vsce": "1.77.0",
    "vscode-languageclient": "6.1.3",
    "webpack": "4.44.0",
    "webpack-cli": "3.3.12"
  }
}
